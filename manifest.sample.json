{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "SideTimeTable",
  "version": "1.5.0",
  "description": "__MSG_appDesc__",
  "permissions": ["sidePanel", "identity", "storage"],
  "oauth2": {
    "client_id": "FILL_OUT_YOUR_CLIENT_ID",
    "scopes": ["https://www.googleapis.com/auth/calendar.readonly"]
  },
  "key": "FILL_OUT_YOUR_KEY",
  "action": {
    "default_popup": "src/side_panel/side_panel.html"
  },
  "side_panel": {
    "default_path": "src/side_panel/side_panel.html"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "options_page": "src/options/options.html",
  "icons": {
    "16": "src/img/icon16.png",
    "32": "src/img/icon32.png",
    "48": "src/img/icon48.png",
    "128": "src/img/icon128.png"
  },
  "commands": {
    "open-side-panel": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "__MSG_toggleSidePanelShortcut__"
    }
  }
}
